<p style="color: green"><%= notice %></p>

<h1>Wallpapers</h1>

<%= link_to "New wallpaper", new_admin_wallpaper_path, class: "btn btn-primary" %>
<%= link_to "New collection", new_admin_collection_path, class: "btn btn-success" %>

<div id="wallpapers">
  <% @wallpapers.each do |wallpaper| %>
    <%= render wallpaper %>
  <% end %>
</div>
<div> <%= paginate @wallpapers %> </div>
