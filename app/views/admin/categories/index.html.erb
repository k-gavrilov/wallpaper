<h1>Categories</h1>
<table class="table">
  <tr>
    <th>Category</th>
    <th>Actions</th>
  </tr>
  <% @categories.each do |category| %>
    <tr>
      <td><%= category.name %></td>
      <td>
        <%= render partial: 'actions', locals: {category: category} %>
      </td>
    </tr>
  <% end %>
</table>
